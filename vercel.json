{
    "version": 2,
    "functions": {
      "api/handleUSSD.js": {
        "memory": 128,
        "maxDuration": 10
      }
    },
    "routes": [
      {
        "src": "/api/ussd",
        "dest": "/api/ussd.js"
      }
    ],
    "env": {
      "KV_REST_API_URL": "@kv_rest_api_url",
      "KV_REST_API_TOKEN": "@kv_rest_api_token"
    }
  }